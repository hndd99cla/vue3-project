{
  "compilerOptions": {
    "strict": true,
    "downlevelIteration": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true,
    "lib": ["esnext", "dom"],
    "allowSyntheticDefaultImports": true,
    "allowUmdGlobalAccess": true,
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ]
}
